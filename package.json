{
  "name": "medinsky",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently --kill-others \"npm:start:frontend\" \"npm:start:backend\"",
    "start:frontend": "ng serve",
    "start:backend": "node dist/server.js",
    "build": "ng build --prod --build-optimizer && yarn build:backend",
    "build:backend": "tsc -p \"backend/tsconfig.json\" && copyfiles -u 1 backend/users.json dist/",
    "ng": "ng",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/frontend/jest.conf.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "frontend/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/material": "6.4.7",
    "@angular/material-moment-adapter": "6.4.7",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@ngrx/store": "6.1.2",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "body-parser": "1.18.3",
    "bootstrap": "4.1.3",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.4",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "immutable": "3.8.2",
    "is-md5": "0.0.2",
    "jsonwebtoken": "8.3.0",
    "md5": "2.2.1",
    "moment": "2.22.2",
    "rxjs": "6.2.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "7.4.4",
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "6.2.9",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@auth0/angular-jwt": "2.0.0",
    "@ngrx/effects": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/express-jwt": "0.0.40",
    "@types/jasmine": "2.8.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "23.3.9",
    "@types/jsonwebtoken": "7.2.8",
    "@types/md5": "2.1.33",
    "@types/node": "8.9.4",
    "@types/shelljs": "0.8.0",
    "@types/webpack": "4.4.13",
    "codelyzer": "4.3.0",
    "concurrently": "4.0.1",
